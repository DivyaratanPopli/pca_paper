	keywords = {Amino Acid Substitution, Animals, Croatia, DNA, Exome, Gene Frequency, Genetic Variation, Humans, Neanderthals, Paleontology, Phylogeny, Polymorphism, Single Nucleotide, Siberia, Spain, ancient DNA, exome capture, paleogenetics, site frequency spectra},
